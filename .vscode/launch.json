{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            // "args": ["server"],  // 参数
            "justMyCode": false,    //debug包中的代码
            "env": {
                "PYTHONUNBUFFERED": "1",    // 确保输出不会被缓冲
                "PYDEVD_WARN_EVALUATION_TIMEOUT": "120",    // 避免debug console超时
            }
        }
    ]
}